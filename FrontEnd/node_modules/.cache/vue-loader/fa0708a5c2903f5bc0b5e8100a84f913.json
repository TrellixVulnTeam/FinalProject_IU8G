o4\"\r\n                value=\"4\"\r\n                aria-label=\"...\"\r\n                v-model=\"value\"\r\n              />\r\n              <br />\r\n              <label for=\"inlineRadio4\">4</label>\r\n            </b-col>\r\n            <b-col class=\"foreword\">\r\n              <input\r\n                type=\"radio\"\r\n                name=\"inlineRadioOptions\"\r\n                id=\"inlineRadio5\"\r\n                value=\"5\"\r\n                aria-label=\"...\"\r\n                v-model=\"value\"\r\n              />\r\n              <br />\r\n              <label for=\"inlineRadio5\">5</label>\r\n            </b-col>\r\n            <b-col class=\"foreword\">\r\n              <input\r\n                type=\"radio\"\r\n                name=\"inlineRadioOptions\"\r\n                id=\"inlineRadio6\"\r\n                value=\"6\"\r\n                aria-label=\"...\"\r\n                v-model=\"value\"\r\n              />\r\n              <br />\r\n              <label for=\"inlineRadio6\">6</label>\r\n            </b-col>\r\n            <b-col class=\"foreword\">\r\n              <input\r\n                type=\"radio\"\r\n                name=\"inlineRadioOptions\"\r\n                id=\"inlineRadio7\"\r\n                value=\"7\"\r\n                aria-label=\"...\"\r\n                v-model=\"value\"\r\n              />\r\n              <br />\r\n              <label for=\"inlineRadio7\">7</label>\r\n            </b-col>\r\n            <b-col class=\"foreword\">\r\n              <input\r\n                type=\"radio\"\r\n                name=\"inlineRadioOptions\"\r\n                id=\"inlineRadio8\"\r\n                value=\"8\"\r\n                aria-label=\"...\"\r\n                v-model=\"value\"\r\n              />\r\n              <br />\r\n              <label for=\"inlineRadio8\">8</label>\r\n            </b-col>\r\n            <b-col class=\"foreword\">\r\n              <input\r\n                type=\"radio\"\r\n   