A,CAAA,KAAA,CAAA,KAAA,CAAA,oBAAA,GAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,eAFA;AAAA;AAAA;AAAA;;AAGA,cAAA,KAAA,CAAA,qDAAA,CAAA;;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,UAAA;;AAJA;AAAA;;AAAA;AAAA;AAAA;AAAA,qBASA,KAAA,CAAA,KAAA,CAAA,GAAA,CACA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,QAAA,GACA,qBAFA,CATA;;AAAA;AASA,cAAA,YATA;AAaA,cAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,kBAAA,YAAA,CAAA,MAAA,KAAA,GAAA,EAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,WAAA;AAdA;AAAA;;AAAA;AAAA;AAAA;AAgBA,cAAA,OAAA,CAAA,GAAA,CAAA,2BAAA,EAAA,YAAA,YAAA,CAAA,MAAA;;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,WAAA;;AAjBA;;AAAA;AAoBA;AACA;AACA;AACA;AACA,cAAA,KAAA,CAAA,cAAA,GAAA,YAAA,CAAA,IAAA,CAAA,CAAA,EAAA,cAAA;AAxBA;AAAA;AAAA,qBA2BA,KAAA,CAAA,KAAA,CAAA,GAAA,CACA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,QAAA,GACA,iCADA,GACA,KAAA,CAAA,cAFA,CA3BA;;AAAA;AA2BA,cAAA,QA3BA;AA+BA,cAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,kBAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,WAAA;AAhCA;AAAA;;AAAA;AAAA;AAAA;AAkCA,cAAA,OAAA,CAAA,GAAA,CAAA,uBAAA,EAAA,YAAA,QAAA,CAAA,MAAA;;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,WAAA;;AAnCA;;AAAA;AAuCA,cAAA,MAvCA,GAuCA,QAAA,CAAA,IAvCA;;AAwCA,mBAAA,GAAA,IAAA,MAAA,EAAA;AACA,gBAAA,IADA,GACA;AAAA,kBAAA,GAAA,EAAA,MAAA,CAAA,GAAA,CAAA,CAAA,QAAA;AACA,kBAAA,eAAA,EAAA,MAAA,CAAA,GAAA,CAAA,CAAA,KADA;AAEA,kBAAA,UAAA,EAAA;AAFA,iBADA;;AAIA,gBAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA;AACA;;AA7CA;AAAA;AAAA;;AAAA;AAiDA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;;AAjDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmDA;AA7EA,CAAA","sourcesContent":["<template>\r\n  <div class=\"d-flex flex-column justify-center align-center\">\r\n    <div class=\"d-flex flex-row justify-center align-center\">\r\n      <v-select-image\r\n        class=\"w-650\"\r\n        v-model=\"selectedItems\"\r\n        :items=\"items\"\r\n        :colorSchema=\"colorSchema\"\r\n        uniqueKey=\"key\"\r\n        :maxSelectable=\"maxSelectable\"\r\n        @maxSelectionError=\"maxSelected()\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport VSelectImage from \"../components/VSelectImage.vue\";\r\nexport default {\r\n  name: \"App\",\r\n  components: { VSelectImage },\r\n  methods: {\r\n    maxSelected() {\r\n      alert(\"You cant select more than \" + this.maxSelectable);\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      maxSelectable: 2,\r\n      selectedItems: [],\r\n      items: [],\r\n      colorSchema: \"#8B8B8B\",\r\n    };\r\n  },\r\n  watch: {\r\n    maxSelectable(newVal, oldVal) {\r\n      if (newVal < this.selectedItems.length) {\r\n        this.maxSelectable = oldVal;\r\n      }\r\n      if (this.items.length < newVal) {\r\n        this.maxSelectable = oldVal;\r\n      }\r\n    },\r\n  },\r\n  async created() {\r\n      if(this.$root.store.email){\r\n        if(this.$root.store.numberOfImagesRating < this.$root.store.minImagesRating){\r\n          alert(\"You did not rate enough photos, continue to rate...\");\r\n          this.$router.push(\"/ranking\");\r\n        }\r\n        else{\r\n            let paramsOfGame;\r\n            try {\r\n                paramsOfGame = await this.axios.get(\r\n                this.$root.store.base_url +\r\n                    \"/users/getAllParams\"\r\n                );\r\n                console.log(paramsOfGame);\r\n                if (paramsOfGame.status !== 200) this.$router.replace(\"/NotFound\");\r\n            } catch (error) {\r\n                console.log(\"error.paramsOfGame.status\", error.paramsOfGame.status);\r\n                this.$router.replace(\"/NotFound\");\r\n                return;\r\n            }\r\n            // const limit = paramsOfGame.data[0].images_selectes_in_game;\r\n            // console.log(limit);\r\n            // this.maxSelectable = limit;\r\n            // console.log(this.maxSelectable);\r\n            this.numberOfImages = paramsOfGame.data[0].images_in_game;\r\n            let response;\r\n            try {\r\n                response = await this.axios.get(\r\n                this.$root.store.base_url +\r\n                    \"/users/getImagesForGame/amount/\"+this.numberOfImages\r\n                );\r\n                console.log(response);\r\n                if (response.status !== 200) this.$router.replace(\"/NotFound\");\r\n            } catch (error) {\r\n                console.log(\"error.response.status\", error.response.status);\r\n                this.$router.replace(\"/NotFound\");\r\n                return;\r\n            }\r\n\r\n            const images = response.data;\r\n            for(let num in images){\r\n              const data = {key : images[num].image_id,\r\n                            backgroundImage: images[num].image,\r\n                            selectable: true};\r\n              this.items.push(data);\r\n            }\r\n        }\r\n        }\r\n        else{\r\n            this.$router.push(\"/login\");\r\n        }\r\n    },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.d-flex {\r\n  display: flex;\r\n}\r\n.flex-row {\r\n  flex-direction: row;\r\n}\r\n.flex-column {\r\n  flex-direction: column;\r\n}\r\n.justify-center {\r\n  justify-content: center;\r\n}\r\n.align-center {\r\n  align-items: center;\r\n}\r\n.w-650 {\r\n  max-width: 650px;\r\n}\r\n.mt-20 {\r\n  margin-top: 20px;\r\n}\r\n.mt-25 {\r\n  margin-top: 20px;\r\n}\r\npre {\r\n  counter-reset: line-numbering;\r\n  background: #2c3e50;\r\n  padding: 12px 0px 14px 0;\r\n  width: 600px;\r\n  color: #ecf0f1;\r\n  line-height: 100%;\r\n}\r\npre code .line::before {\r\n  content: counter(line-numbering);\r\n  counter-increment: line-numbering;\r\n  padding-right: 1em;\r\n  /* space after numbers */\r\n  padding-left: 8px;\r\n  width: 1.5em;\r\n  text-align: right;\r\n  opacity: 0.5;\r\n  color: white;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/pages"}]}